<?xml version="1.0"?>
<block>
	<name>OP25 FSK4 Demodulator</name>
	<key>op25_fsk4</key>
	<!--<category>Synchronizers</category>-->
	<import>from baz import baz_op25</import>
	<make>baz_op25.op25_fsk4(channel_rate=$channel_rate, #slurp
#if $tune_out()
auto_tune_msgq=$(id)_msgq_out,#slurp
#end if
)</make>
	
	<!--<callback></callback>-->
	
	<param>
	<name>Channel Rate</name>
	<key>channel_rate</key>
	<type>real</type>
	</param>

	<param>
		<name>Output auto tune</name>
		<key>tune_out</key>
		<value>True</value>
		<type>bool</type>
		<hide>part</hide>
		<option>
			<name>Yes</name>
			<key>True</key>
		</option>
		<option>
			<name>No</name>
			<key>False</key>
		</option>
	</param>
	
	<!--<check>not $win_size or len($win_size) == 2</check>-->
	
	<sink>
		<name>bb</name>
		<type>float</type>
	</sink>
	
	<source>
		<name>dibits</name>
		<type>float</type>
	</source>
	
	<source>
		<name>tune</name>
		<type>msg</type>
		<optional>1</optional>
	</source>
	
	<doc>OP25 C4FM (FSK4)

Input real floating-point baseband data.
Outputs synchronised dibits at channel rate.
	</doc>
</block>
